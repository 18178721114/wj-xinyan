CREATE TABLE `custom_fields` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `org_id` int(11) NOT NULL,
  `type` tinyint(1) NOT NULL COMMENT '1、填空；2、单选；3、多选',
  `interactive_mode` tinyint(1) NOT NULL COMMENT '1、输入；2、下拉；3、平铺；',
  `title` varchar(100) NOT NULL,
  `placeholder` varchar(100) NOT NULL,
  `options` json DEFAULT NULL,
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '0、删除；1、正常使用；',
  `show` tinyint(1) NOT NULL DEFAULT '0' COMMENT '0、不在报告中展示；1、在报告中展示；',
  `created` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `required` tinyint(1) NOT NULL DEFAULT '0' COMMENT '必填：0、不必填；1、必填',
  PRIMARY KEY (`id`),
  KEY `custom_fields_org_id_IDX` (`org_id`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=10001 DEFAULT CHARSET=utf8

