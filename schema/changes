   `product_type` tinyint(1) DEFAULT NULL COMMENT '产品类型：1：健康，2：医疗，3视光',
   `status` tinyint(1) DEFAULT NULL COMMENT '状态0：停用、1:正常',
   `created` datetime DEFAULT CURRENT_TIMESTAMP,
   `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
   PRIMARY KEY (`group_id`),
   KEY `item_id` (`item_id`) USING BTREE
 ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='项目文案组';

 CREATE TABLE `yt_cms_item_detail` (
   `detail_id` int(11) NOT NULL AUTO_INCREMENT,
   `group_id` int(11) DEFAULT NULL COMMENT 'yt_cms_item_group表主键id',
   `type_id` int(11) DEFAULT NULL COMMENT 'yt_cms_type表主键id',
   `content` text DEFAULT NULL COMMENT '文案内容',
   `created` datetime DEFAULT CURRENT_TIMESTAMP,
   `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
   PRIMARY KEY (`detail_id`),
   KEY `group_id` (`group_id`) USING BTREE
 ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='项目文案详情';

CREATE TABLE `yt_cms_type` (
  `type_id` int(11) NOT NULL AUTO_INCREMENT,
  `type` int(11) DEFAULT NULL COMMENT '文案类型',
  `title` VARCHAR(10) DEFAULT NULL COMMENT '类型名称',
  `key` VARCHAR(50) DEFAULT NULL COMMENT 'key',
  `content` JSON DEFAULT NULL COMMENT '文案结构',
  `created` datetime DEFAULT CURRENT_TIMESTAMP,
  `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`type_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='项目文案类型';

 CREATE TABLE `yt_cms_tag` (
   `tag_id` int(11) NOT NULL AUTO_INCREMENT,
   `title` VARCHAR(20) DEFAULT NULL COMMENT '标签名称',
   `key` VARCHAR(20) DEFAULT NULL COMMENT '标签key',
   `created` datetime DEFAULT CURRENT_TIMESTAMP,
   `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
   PRIMARY KEY (`tag_id`)
 ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='展示文案标签表';

 CREATE TABLE `yt_cms_tag_item_detail_map` (
   `tag_id` int(11) NOT NULL COMMENT 'yt_tag表主键id',
   `detail_id` int(11) NOT NULL COMMENT 'item_detail表主键id',
   `created` datetime DEFAULT CURRENT_TIMESTAMP,
   `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
   KEY `tag_id` (`tag_id`,`detail_id`) USING BTREE
 ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='展示文案标签表';

 CREATE TABLE `yt_cms_config` (
   `cms_conf_id` int(11) NOT NULL AUTO_INCREMENT,
   `owner_id` int(11) DEFAULT NULL COMMENT 'owner_type所属表的主键id',
   `owner_type` tinyint(4) DEFAULT NULL COMMENT '类型 1:上传机构2上传用户',
   `version` varchar(10) DEFAULT NULL COMMENT '版本号',
   `product_type` tinyint(1) DEFAULT NULL COMMENT '产品类型：1：健康，2：医疗，3视光',
   `config` json DEFAULT NULL COMMENT 'yt_json_field表中字段',
   `created` datetime DEFAULT CURRENT_TIMESTAMP,
   `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
   PRIMARY KEY (`cms_conf_id`),
   KEY `owner_id` (`owner_id`) USING BTREE
 ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='机构所选的文案配置';

 CREATE TABLE `yt_check_info` (
   `check_id` int(11) DEFAULT NULL COMMENT 'check_info表主键id',
   `cms_conf_id` int(11) DEFAULT NULL COMMENT 'yt_cms_config表主键id',
   `check_type` tinyint(4) DEFAULT NULL COMMENT '检查类型1:眼底,2:护心宝',
   `extra` json DEFAULT NULL COMMENT 'yt_json_field表中字段',
   `created` datetime DEFAULT CURRENT_TIMESTAMP,
   `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
   KEY `check_id` (`check_id`) USING BTREE,
   KEY `cms_conf_id` (`cms_conf_id`) USING BTREE
 ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='检查单与文案组map关系';

 CREATE TABLE `yt_json_field` (
   `id` int(11) NOT NULL AUTO_INCREMENT,
   `key` varchar(20) DEFAULT NULL COMMENT 'json字段key',
   `key_type` tinyint(1) DEFAULT NULL COMMENT '存入字段类型：1:int,2:float,3:bool,4:string,5:array,6:object',
   `table_name` varchar(50) DEFAULT NULL COMMENT '数据库表名',
   `field_name` varchar(50) DEFAULT NULL COMMENT '表名所在的字段名称',
   `comment` varchar(50) DEFAULT NULL COMMENT 'json字段解释',
   `created` datetime DEFAULT CURRENT_TIMESTAMP,
   `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
   PRIMARY KEY (`id`)
 ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='json结构中的字段';
