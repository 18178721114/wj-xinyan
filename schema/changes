CREATE TABLE `wechat_config` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `appid` varchar(100) NOT NULL,
  `token` varchar(100) NOT NULL DEFAULT '',
  `prefix` char(5) NOT NULL DEFAULT '',
  `secret` varchar(100) NOT NULL,
  `name` varchar(100) NOT NULL,
  `product_type` tinyint(1) NOT NULL DEFAULT '-1' COMMENT '产品类型-1:其他;1慧心瞳;2:鹰瞳扫描3:众佑4:体知健',
  `type` tinyint(1) NOT NULL DEFAULT '1' COMMENT '类型 1 公众号 2 小程序 3 微信支付',
  `relation_applet` int(11) NOT NULL DEFAULT '0' COMMENT '0 代表不关联小程序  ，其他wechat_config表中id',
  `relation_payment` int(11) NOT NULL DEFAULT '0' COMMENT '0 代表不关联微信支付  ，其他wechat_config表中id',
  PRIMARY KEY (`id`),
  KEY `wechat_config_prefix_IDX` (`prefix`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8

