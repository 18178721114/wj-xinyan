CREATE TABLE `remote_check_result` (
  `check_id` int(11) NOT NULL AUTO_INCREMENT COMMENT '检查单id',
  `org_id` int(11) NOT NULL DEFAULT '0' COMMENT '机构id',
  `third_id` varchar(64) NOT NULL DEFAULT '' COMMENT '合作伙伴数据id',
  `created` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `updated` datetime DEFAULT '1971-01-01 00:00:00' ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  `status` tinyint(4) DEFAULT '1' COMMENT '状态(1有效；0无效)',
  `left_result` varchar(255) NOT NULL DEFAULT '' COMMENT '左眼诊断结果(举例:豹纹样改变，动脉弹性减弱)',
  `right_result` varchar(255) NOT NULL DEFAULT '' COMMENT '右眼诊断结果(举例:地图样萎缩，动脉弹性减弱)',
  `left_symptom` varchar(255) NOT NULL DEFAULT '' COMMENT '左眼影像描述,逗号分割句号结尾(举例:视网膜色素上皮减少，可见脉络膜血管暴露，豹纹样改变明显。)',
  `right_symptom` varchar(255) NOT NULL DEFAULT '' COMMENT '右眼影像描述,逗号分割句号结尾(举例:影像显示黄斑区脉络膜萎缩，视网膜色素上皮缺失，提示地图样萎缩可能。)',
  `left_quality` varchar(255) NOT NULL DEFAULT '' COMMENT '左眼图片质量评估(举例:图片不可读、图片模糊、图片质量合格)',
  `right_quality` varchar(255) NOT NULL DEFAULT '' COMMENT '右眼图片质量评估(举例:图片不可读、图片模糊、图片质量合格)',
  `review_changed` tinyint(4) NOT NULL DEFAULT '0' COMMENT '审核状态(1审核有修改；2直接通过；0无修改)',
  `result` json NULL COMMENT '审核结果',
  PRIMARY KEY (`check_id`),
  KEY `org_id` (`org_id`),
  KEY `created` (`created`),
  KEY `third_id` (`third_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='患者三方系统远程阅片信息';

