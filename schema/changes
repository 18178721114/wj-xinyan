  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '机构与产品关系ID',
  `org_id` int(11) NOT NULL DEFAULT '0' COMMENT '机构ID',
  `package_id` int(11) NOT NULL DEFAULT '0' COMMENT '产品套餐ID',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '状态：0删除，1启用',
  `create_user_id` int(11) NOT NULL DEFAULT '0' COMMENT '创建人ID',
  `update_user_id` int(11) NOT NULL DEFAULT '0' COMMENT '修改人ID',
  `created` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `updated` datetime NOT NULL DEFAULT '1971-01-01 00:00:00' ON UPDATE CURRENT_TIMESTAMP COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `product_package` (
  `package_id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '产品ID',
  `package_name` varchar(128) NOT NULL DEFAULT '' COMMENT '产品名称',
  `product` varchar(32) NOT NULL DEFAULT 'huixintong' COMMENT 'huixintong,yingtong,zhonogyou,tizhijian',
  `status` tinyint(1) NOT NULL DEFAULT '1' COMMENT '产品状态0失效，1生效，2锁定',
  `package_first` tinyint(1) unsigned DEFAULT '1' COMMENT '1：东海；2：秦岭；3：太行；4: 昆仑；5：眼科；',
  `config` json NOT NULL COMMENT '产品配置',
  `descriptions` text COMMENT '套餐注解',
  `create_user_id` int(10) DEFAULT '0' COMMENT '创建人id',
  `update_user_id` int(10) DEFAULT '0' COMMENT '修改人id',
  `created` datetime DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `updated` datetime DEFAULT '1971-01-01 00:00:00' ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`package_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='套餐、产品配置表';

