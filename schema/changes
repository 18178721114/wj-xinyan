#用户协议
#drop table yt_user_agreement;
CREATE TABLE `yt_user_agreement` (
  `user_agreement_id` int(11) NOT NULL AUTO_INCREMENT,
  `org_id` int(11) DEFAULT NULL COMMENT '机构id',
  `agreement_type` tinyint(1) DEFAULT NULL COMMENT '1鹰瞳提供模版2 客户自定义3鹰瞳用户协议',
  `company_name` varchar(255) DEFAULT NULL COMMENT '公司全称',
  `company_alias` varchar(255) DEFAULT NULL COMMENT '公司简称',
  `customer_service_contact` varchar(20) DEFAULT NULL COMMENT '联系方式',
  `company_logo` varchar(255) DEFAULT NULL COMMENT '公司logo',
  `user_agreement_url` varchar(255) DEFAULT NULL COMMENT '用户协议url',
  `status` tinyint(1) DEFAULT '1' COMMENT '0失效 1生效',
  `privacy_agreement_url` varchar(255) DEFAULT NULL COMMENT '隐私协议 url',
  `created` datetime DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `updated` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
  `user_agreement_image` varchar(2000) DEFAULT NULL COMMENT '用户协议图片逗号分隔',
  `privacy_agreement_image` varchar(2000) DEFAULT NULL COMMENT '隐私协议图片逗号分隔',
  PRIMARY KEY (`user_agreement_id`) USING BTREE,
  KEY `org_id` (`org_id`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=0 DEFAULT CHARSET=utf8mb4;

