  `so2` smallint(11) NOT NULL COMMENT '血氧',
  `r` smallint(11) NOT NULL COMMENT '呼吸',
  `t` float NOT NULL COMMENT '体温',
  `self_risk` tinyint(4) NOT NULL COMMENT '感染风险',
